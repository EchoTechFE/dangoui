{
  "name": "@echoingtech/dumpling",
  "version": "1.0.0-alpha.9",
  "scripts": {
    "build": "npm run build-lib && npm run build-packages",
    "build-lib": "vue-cli-service build --target lib --name DumplingUI --filename index src/index.js",
    "build-packages": "node scripts/build-packages.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/echoingtech/dumpling.git"
  },
  "files": [
    "dist/*",
    "packages",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "@echoingtech/du-button": "workspace:*",
    "@echoingtech/du-form": "workspace:*",
    "@echoingtech/du-input": "workspace:*",
    "@echoingtech/du-input-number": "workspace:*",
    "@echoingtech/du-popup": "workspace:*",
    "@echoingtech/du-radio": "workspace:*",
    "@echoingtech/du-steps": "workspace:*",
    "@echoingtech/du-styles": "workspace:*",
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/preset-scss": "~1.0.3",
    "@storybook/vue": "^6.4.19",
    "@vue/cli-plugin-babel": "~4.5.6",
    "@vue/cli-service": "~4.5.6",
    "babel-loader": "^8.2.3",
    "postcss-loader": "4",
    "postcss-px-to-viewport": "~1.1.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.11"
  }
}
