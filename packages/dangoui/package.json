    "ali-oss": "^6.18.1",
    "font-carrier": "^0.3.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "happy-dom": "^13.6.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "nodemon": "^3.1.0",
