  "files": [
    "dist",
    "src",
    "typings"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./typings/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./platte": {
